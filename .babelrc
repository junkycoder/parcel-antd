{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    [
      "@babel/preset-react"
    ]
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "@ant-design/icons",
        "libraryDirectory": "es/icons",
        "camel2DashComponentName": false
      },
      "@ant-design/icons"
    ],
    ["import", { "libraryName": "antd-mobile", "style": "css" }],
    "@babel/plugin-transform-flow-strip-types"
  ]
}